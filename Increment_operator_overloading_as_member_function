class Increment{
    int value;
    public:
    Increment(){
        value=0;
    }
    Increment(int x){
        value=x;
    }
    void putData(){
        cout<<"value is "<<value<<endl;
    }
    Increment operator ++(){
        Increment Incr;
        value=value+1;
        Incr.value=value;
        return Incr;
    }
    Increment operator ++(int obj){
        Increment Incr;
        Incr.value=value;
        value=value+1;
        return Incr;
    }

};
int main(){
    Increment i(18), j;
    i.putData();
    j=i++;
    i.putData();
    j.putData();
    j=++i;
    i.putData();
    j.putData();
}
